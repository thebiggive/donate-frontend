    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-browser-dynamic": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.0",
    "@biggive/components": "^202509221119.0.0",
    "@biggive/components-angular": "^202509221119.0.0",
    "@fortawesome/angular-fontawesome": "^2.0.1",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.6.0",
    "@fortawesome/pro-solid-svg-icons": "^6.6.0",
    "@stripe/stripe-js": "^7.3.1",
    "browserslist-useragent-regexp": "^4.1.3",
    "compression": "^1.8.1",
    "detect-browser": "^5.3.0",
    "eslint-plugin-compat": "^6.0.2",
    "express": "^4.18.2",
    "friendly-challenge": "^0.9.16",
    "helmet": "^7.1.0",
    "jwt-decode": "^3.1.2",
    "material-icons-font": "^2.1.0",
    "morgan": "^1.10.1",
    "ngx-cookie-service": "^20.0.1",
    "ngx-infinite-scroll": "^20.0.0",
    "ngx-matomo-client": "^8.0.0",
    "ngx-webstorage-service": "^5.0.0",
    "rxjs": "^7.8.1",
    "swiper": "^11.0.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.0",
    "@angular/build": "^20.3.0",
    "@angular/cli": "^20.3.0",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/language-service": "^20.3.0",
    "@babel/cli": "^7.28.3",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "7.27.6",
    "@cypress/schematic": "^2.5.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^3.6.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/morgan": "^1.9.2",
    "@types/node": "^20.13.1",
    "angular-eslint": "20.1.0",
    "browser-sync": "^3.0.0",
    "cypress": "^13.2.0",
    "eslint": "^9.28.0",
    "eslint-plugin-no-floating-promise": "^2.0.0",
    "jasmine-core": "^3.99.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
