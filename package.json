{
  "name": "donate-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-with-backend": "(cd ../matchbot/; docker compose up -d) && (cd ../identity/; docker compose up -d) &&  ng serve",
    "start-dangerously": "ng serve --host 0.0.0.0",
    "test": "ng test",
    "test-ci": "ng test --karma-config ./karma-ci.conf.js --browsers=ChromeHeadlessNoSandbox",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build": "npm run supportedBrowsers && ng build",
    "build:staging": "ng build --configuration staging",
    "build:regression": "ng build --configuration regression",
    "build:production": "ng build --configuration production",
    "ci": "npm run supportedBrowsers && npm run lint && npx prettier . --check && npm run test-ci && npm run e2e && npm run build:staging",
    "link-components": "./link-components.sh",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "supportedBrowsers": "(echo export const supportedBrowsers: RegExp = && browserslist-useragent-regexp --allowHigherVersions) > src/supportedBrowsers.ts",
    "prettier": "prettier . --write --list-different",
    "create-legacy-build": "node scripts/create-legacy-build.js",
    "serve:ssr": "node dist/donate-frontend/server/server.mjs"
  },
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
