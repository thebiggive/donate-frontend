    "ts-node": "^10.9.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.33.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.5.0"
  },
  "engines": {
    "node": ">=22.12",
    "npm": ">=10"
  }
